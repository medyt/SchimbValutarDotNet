@model IEnumerable<Domain_Entities.Valuta>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AcronimValuta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DenumireValuta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Multiplu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumarCurentValuta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PozitieValuta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipValuta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AcronimValuta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DenumireValuta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Multiplu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumarCurentValuta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PozitieValuta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipValuta)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
